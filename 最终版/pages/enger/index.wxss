.bg{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0px; left: 0px; 
  background-size:100% 100%;
  /*z-index 较大的元素会覆盖较小的元素在上层进行显示*/
  z-index: -1;
  filter: blur(1rpx);
}
